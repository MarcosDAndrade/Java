package Projeto;

public abstract class Pessoa {
	// ATRIBUTOS
	protected String nome;
	protected String telefone;
	protected String email;
	
	// CONSTRUTORES
	Pessoa(){}
	Pessoa(String n, String t, String e){
		this.nome = n;
		this.telefone = t;
		this.email = e;
	}
	
	// METODOS
	public void imprimeDados() {

	}
	
}
